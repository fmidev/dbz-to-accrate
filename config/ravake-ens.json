{
  "coef": {
    "zr_a": 223.0,
    "zr_b": 1.53,
    "zr_a_c": 223.0,
    "zr_b_c": 1.43,
    "zs_a": 115,
    "zs_b": 1.35
  },

  "input": {
    "deterministic": {
      "data": {
        "filetype": "deterministic_forecast",
        "dir": "/input",
        "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.det_conf=ravake-det.h5",
        "timeres": 1,
        "fc_len": 140
      },
      "motion": {
        "filetype": "motion",
        "dir": "/input",
        "filename": "{timestamp}_radar.fmippn.motion_conf={config}.h5"
      }
    },
    "ensemble": {
      "data": {
        "filetype": "ensemble_forecast",
        "dir": "/input",
        "filename": "{timestamp}00_{fc_timestamp}00_radar.fmippn.ens_conf={config}_ensmem={ensno}.h5",
        "timeres": 5,
        "fc_len": 140,
        "n_ens_members": 5,
        "allow_n_members_missing": 1
      },
      "motion": {
        "filetype": "motion",
        "dir": "/input",
        "filename": "{timestamp}_radar.fmippn.motion-ensmem={ensno}_conf={config}.h5"
      }
    },
    "observations": {
      "data": {
        "filetype": "observation",
        "dir": "/ppn_input",
        "filename": "{timestamp}_radar.rack.comp_CONF=FMIPPN-SMALL,ANDRE.h5",
        "timeres": 5
      }
    },
    "snowprob": {
      "data": {
        "filetype": "snowprob",
        "dir": "/input",
        "filename": "{timestamp}_model.harmonie.snowprob_CONF=RAVAKE.h5",
        "timeres": 60
      }
    }
  },
  "interp": {
    "interpolate": true,
    "timeres": 1
  },
  "output": {
    "accumulations": {
      "timeconfig": {
      "write_acrr_from_start": true,
      "write_acrr_fixed_step": true,
      "timestep": 60,
        "timeres": 5,
        "fc_len": 140
      },
      "ensemble_mean": {
        "dir": "/output",
        "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.accrate.ens_mean.{acc_timestep}min_conf={config}.h5",
        "dtype": "uint16",
        "gain": 0.001,
        "offset": 0,
        "nodata": 65535,
        "undetect": 0
      },
      "deterministic": {
        "dir": "/output",
        "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.accrate.det.{acc_timestep}min_conf={config}.h5",
        "dtype": "uint16",
        "gain": 0.001,
        "offset": 0,
        "nodata": 65535,
        "undetect": 0
      },
      "weighted_mean_det_ens": {
        "dir": "/output",
        "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.accrate.ens_det_mean.{acc_timestep}min_conf={config}.h5",
      "dtype": "uint16",
      "gain": 0.001,
      "offset": 0,
      "nodata": 65535,
      "undetect": 0,
        "determ_initweight": 120,
        "determ_weightspan": 150
      }
    },
    "interpolation": {
      "dir": "/output",
      "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.interp_conf={config}_ensmem={ensno}.h5",
      "dtype": "uint16",
      "gain": 0.001,
      "offset": 0,
      "nodata": 65535,
      "undetect": 0
    },
    "rate": {
      "dir": "/output",
      "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.rate_conf={config}_ensmem={ensno}.h5",
      "dtype": "uint16",
      "gain": 0.001,
      "offset": 0,
      "nodata": 65535,
      "undetect": 0
    },
    "nodata": {
      "dir": "/output",
      "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.accrate.nodata.{acc_timestep}min_conf={config}.h5",
      "dtype": "uint8",
      "gain": 1,
      "offset": 0,
      "nodata": 255,
      "undetect": 0
    },
    "undetect": {
      "dir": "/output",
      "filename": "{timestamp}+{fc_timestep}min_radar.fmippn.accrate.undetect.{acc_timestep}min_conf={config}.h5",
      "dtype": "uint8",
      "gain": 1,
      "offset": 0,
      "nodata": 255,
      "undetect": 0
    }
  },
  "multiprocessing": {
    "scheduler": "threads",
    "num_workers": 4
  }
}
